{
   "author": "Bitwarden Inc.",
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon19.png",
         "38": "images/icon38.png"
      },
      "default_popup": "popup/index.html",
      "default_title": "Bitwarden"
   },
   "commands": {
      "_execute_browser_action": {
         "description": "__MSG_commandOpenPopup__",
         "suggested_key": {
            "default": "Ctrl+Shift+Y",
            "linux": "Ctrl+Shift+U"
         }
      },
      "_execute_sidebar_action": {
         "description": "__MSG_commandOpenSidebar__",
         "suggested_key": {
            "default": "Alt+Shift+Y",
            "linux": "Alt+Shift+U"
         }
      },
      "autofill_login": {
         "description": "__MSG_commandAutofillDesc__",
         "suggested_key": {
            "default": "Ctrl+Shift+L"
         }
      },
      "generate_password": {
         "description": "__MSG_commandGeneratePasswordDesc__",
         "suggested_key": {
            "default": "Ctrl+Shift+9"
         }
      },
      "lock_vault": {
         "description": "__MSG_commandLockVaultDesc__"
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content/autofill.js", "content/autofiller.js", "content/notificationBar.js", "content/contextMenuHandler.js" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*" ],
      "run_at": "document_start"
   }, {
      "all_frames": false,
      "js": [ "content/message_handler.js" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "css": [ "content/autofill.css" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_extDesc__",
   "developer": {
      "name": "bitwarden"
   },
   "homepage_url": "https://bitwarden.com",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "96": "images/icon96.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_extName__",
   "optional_permissions": [ "nativeMessaging" ],
   "permissions": [ "tabs", "contextMenus", "storage", "unlimitedStorage", "clipboardRead", "clipboardWrite", "idle", "http://*/*", "https://*/*", "webRequest", "webRequestBlocking" ],
   "short_name": "__MSG_appName__",
   "sidebar_action": {
      "default_icon": "images/icon19.png",
      "default_panel": "popup/index.html?uilocation=sidebar",
      "default_title": "Bitwarden"
   },
   "storage": {
      "managed_schema": "managed_schema.json"
   },
   "update_url": "https://extension-updates.opera.com/api/omaha/update/",
   "version": "2023.8.3",
   "web_accessible_resources": [ "notification/bar.html", "images/icon38.png", "images/icon38_locked.png" ]
}
